{
  "name": "stepper",
  "version": "1.0.0",
  "private": true,
  "description": "Stepper to provide accordion style steps or wizard",
  "main": "src/index.js",
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/material": "5.9.3",
    "classnames": "2.3.1",
    "history": "4.10.1",
    "hoist-non-react-statics": "^3.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-transition-group": "4.4.2",
    "stringcase": "^4.3.1",
    "styled-components": "5.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjones5-sh/Stepper.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run client:build",
    "client:build": "npm run clean && NODE_ENV=production webpack",
    "format": "prettier --check 'src/**/*.js'",
    "format:fix": "prettier --write 'src/**/*.js'",
    "lint": "eslint src ",
    "lint:fix": "eslint src --fix",
    "test:local": "react-scripts test",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": "^16.10.x",
    "npm": "^7.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 4 versions",
      "not ie <= 11",
      "not ie_mob <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSameLine": true
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-async-to-generator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "5.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-formatjs": "^10.3.27",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1"
  }
}
