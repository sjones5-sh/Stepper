{
  "name": "stepper",
  "version": "1.0.0",
  "private": true,
  "description": "Stepper to provide accordion style steps or wizard",
  "main": "src/index.js",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.9.3",
    "classnames": "^2.3.1",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stubhub/phoenix.webapp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stubhub/phoenix.webapp/issues"
  },
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run client:build",
    "client:build": "npm run clean && NODE_ENV=production webpack",
    "format": "prettier --check 'src/**/*.js'",
    "format:fix": "prettier --write 'src/**/*.js'",
    "lint": "eslint src ",
    "lint:fix": "eslint src --fix",
    "test:local": "react-scripts test",
    "test": "jest",
    "storybook": "start-storybook -p 6006 -s public"
  },
  "engines": {
    "node": "^16.10.x",
    "npm": "^7.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 4 versions",
      "not ie <= 11",
      "not ie_mob <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSameLine": true
  },
  "pre-commit": [
    "format",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@formatjs/cli": "^4.3.1",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.1",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.3",
    "@testing-library/user-event": "^13.2.1",
    "babel-plugin-formatjs": "^10.3.9",
    "babel-plugin-react-intl": "^8.2.25",
    "eslint-plugin-formatjs": "^2.17.8",
    "eslint-plugin-import": "^2.24.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "4.46.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^2.5.2"
  }
